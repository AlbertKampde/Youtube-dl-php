{
    "name": "chellem/youtube-downloader",
    "description": "PHP library to download video",
    "keywords": ["youtube", "download"],
    "type": "library",
    "authors": [
        {
            "name": "Chellem"
        }
    ],
    "require": {
        "php": ">=5.4.0",
        "symfony/process": ">=2.3",
        "react/react": "dev-master"
    },
    "scripts":{
        "post-update-cmd":[
            "Youtubedl\\Installation::postUpdate"
        ],
        "post-install-cmd":[
            "Youtubedl\\Installation::postInstall"
        ]
    },
    "autoload": {
        "psr-0": {
            "Youtubedl": "src/"
        }
    }
}