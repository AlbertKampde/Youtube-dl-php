{
    "name": "albertkampde/youtube-downloader",
    "description": "PHP library to download video",
    "keywords": ["youtube", "download"],
    "license": "MIT",
    "type": "library",
    "authors": [
        {"name": "Chellem"}
    ],
    "contributors": [
        {"name": "Albert Campde"}
    ],
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/labzone/Youtube-dl-php"
        }
    ],
    "require": {
        "php": ">=5.5.0",
        "symfony/process": "2.x",
        "react/react": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "4.x"
    },
    "scripts":{
        "post-update-cmd":[
            "Youtubedl\\Installation::postUpdate"
        ],
        "post-install-cmd":[
            "Youtubedl\\Installation::postInstall"
        ]
    },
    "autoload": {
        "psr-0": {
            "Youtubedl": "src/"
        }
    }
}
